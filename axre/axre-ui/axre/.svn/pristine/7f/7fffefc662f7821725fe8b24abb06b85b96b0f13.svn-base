package com.concept.ruleengine.archcomp.exception;

public class AbstractRuntimeException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public AbstractRuntimeException(final String message) {
		super(message);
	}

	public AbstractRuntimeException() {
		super();
	}

	public AbstractRuntimeException(final Exception cause) {
		super(cause);
	}

	public AbstractRuntimeException(final String message,
			final Exception cause) {
		super(message, cause);
	}


}
